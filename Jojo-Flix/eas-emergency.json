{
  "cli": {
    "version": ">= 16.8.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "image": "default",
        "resourceClass": "medium"
      }
    },
    "preview": {
      "android": {
        "buildType": "apk",
        "image": "default",
        "resourceClass": "medium"
      },
      "distribution": "internal"
    },
    "production": {
      "android": {
        "buildType": "apk",
        "autoIncrement": true,
        "image": "default",
        "resourceClass": "large"
      }
    },
    "emergency": {
      "android": {
        "buildType": "apk",
        "image": "default",
        "resourceClass": "large",
        "env": {
          "GRADLE_OPTS": "-Xmx4096m -XX:MaxMetaspaceSize=512m",
          "JAVA_OPTS": "-Xmx4096m"
        },
        "config": "emergency-build.json"
      },
      "distribution": "internal"
    }
  },
  "submit": {
    "production": {}
  }
}